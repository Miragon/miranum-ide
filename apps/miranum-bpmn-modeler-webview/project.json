{
  "name": "miranum-bpmn-modeler-webview",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/miranum-bpmn-modeler-webview/src",
  "tags": [],
  "targets": {
    "observe": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "development",
      "options": {
        "watch": true,
        "outputPath": "dist/apps/miranum-modeler/miranum-bpmn-modeler-webview",
        "configFile": "apps/miranum-bpmn-modeler-webview/vite.config.ts",
        "emptyOutDir": false
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": false,
          "vendorChunk": true
        }
      }
    },
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/miranum-modeler/miranum-bpmn-modeler-webview",
        "configFile": "apps/miranum-bpmn-modeler-webview/vite.config.ts"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/miranum-bpmn-modeler-webview/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/miranum-bpmn-modeler-webview/**/*.ts"]
      }
    }
  }
}
