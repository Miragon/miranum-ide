{
  "name": "miranum-jsonforms",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/miranum-jsonforms/src",
  "projectType": "application",
  "implicitDependencies": [
    "miranum-jsonforms-webview"
  ],
  "targets": {
    "observe-all": {
      "executor": "nx:run-commands",
      "parallel": true,
      "options": {
        "commands": [
          {
            "command": "npx nx observe miranum-jsonforms",
            "prefix": "miranum-jsonforms",
            "color": "yellow"
          },
          {
            "command": "npx nx observe miranum-jsonforms-webview",
            "prefix": "miranum-jsonforms-webview",
            "color": "green"
          }
        ]
      }
    },
    "observe": {
      "executor": "@nrwl/node:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "development",
      "options": {
        "watch": true,
        "outputPath": "dist/apps/miranum-jsonforms",
        "deleteOutputPath": false,
        "main": "apps/miranum-jsonforms/src/main.ts",
        "tsConfig": "apps/miranum-jsonforms/tsconfig.app.json",
        "generatePackageJson": true,
        "webpackConfig": "apps/miranum-jsonforms/custom-webpack.config.js",
        "assets": [
          { "glob": "images/komet.svg", "input": "./", "output": "/assets" },
          { "glob": ".vscodeignore", "input": "./apps/miranum-jsonforms/", "output": "/" },
          { "glob": "LICENSE", "input": "./", "output": "/" }
        ]
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true
        }
      }
    },
    "dont-build-yet": {
      "executor": "@nrwl/node:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/miranum-jsonforms",
        "deleteOutputPath": false,
        "main": "apps/miranum-jsonforms/src/main.ts",
        "tsConfig": "apps/miranum-jsonforms/tsconfig.app.json",
        "generatePackageJson": true,
        "webpackConfig": "apps/miranum-jsonforms/custom-webpack.config.js",
        "assets": [
          { "glob": "images/komet.svg", "input": "./", "output": "/assets" },
          { "glob": "images/kometLogo.png", "input": "./", "output": "/assets" },
          { "glob": ".vscodeignore", "input": "./apps/miranum-jsonforms/", "output": "/" },
          { "glob": "LICENSE", "input": "./", "output": "/" }
        ]
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "sourceMap": false,
          "fileReplacements": [
            {
              "replace": "apps/miranum-jsonforms/src/environments/environment.ts",
              "with": "apps/miranum-jsonforms/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/miranum-jsonforms/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/miranum-jsonforms/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
