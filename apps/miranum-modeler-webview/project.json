{
  "name": "miranum-modeler-webview",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/miranum-modeler-webview/src",
  "tags": [],
  "targets": {
    "observe": {
      "executor": "nx:run-commands",
      "parallel": false,
      "options": {
        "commands": [
          "npx nx observe-webview miranum-modeler-webview",
          "sleep 10 && sed -i --verbose 's/import\"vscode\";//' dist/apps/miranum-modeler/miranum-modeler-webview/index.js"
        ]
      }
    },
    "observe-webview": {
      "executor": "@nrwl/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/miranum-modeler/miranum-modeler-webview",
        "configFile": "apps/miranum-modeler-webview/vite.config.ts",
        "deleteOutputPath": false,
        "watch": {}
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": false,
          "vendorChunk": true
        }
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "parallel": false,
      "options": {
        "commands": [
          "npx nx build-bundle miranum-modeler-webview",
          "sleep 10 && sed -i --verbose 's/import\"vscode\";//' dist/apps/miranum-modeler/miranum-modeler-webview/index.js"
        ]
      }
    },
    "build-bundle": {
      "executor": "@nrwl/vite:build",
      "defaultConfiguration": "production",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/miranum-modeler/miranum-modeler-webview"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production",
          "filenameHahsing": false,
          "productionSourceMap": true
        }
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/miranum-modeler-webview/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/miranum-modeler-webview/**/*.ts"]
      }
    }
  }
}
