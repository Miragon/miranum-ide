{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "npmScope": "miranum-ide",
  "affected": {
    "defaultBase": "main"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e"
        ],
        "accessToken": "NjZhNTdlY2EtNTg1Ny00OWQ5LWI0NzYtOWI3Y2M4MmUyOTRjfHJlYWQtd3JpdGU="
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ]
    }
  },
  "workspaceLayout": {
    "appsDir": "apps",
    "libsDir": "libs"
  },
  "defaultProject": "miranum-cli",
  "plugins": [
    "@nxrocks/nx-spring-boot"
  ],
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "css",
        "unitTestRunner": "jest",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "css"
      },
      "library": {
        "style": "css",
        "unitTestRunner": "jest",
        "linter": "eslint"
      }
    }
  }
}
