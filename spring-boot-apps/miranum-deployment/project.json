{
  "name": "miranum-deployment",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nxrocks/nx-spring-boot:build",
      "options": {
        "root": ""
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "java -jar miranum-deployment-proxy-example/target/miranum-deployment-example.jar"
        ],
        "cwd": "spring-boot-apps/miranum-deployment"
      }
    },
    "test": {
      "executor": "@nxrocks/nx-spring-boot:test",
      "options": {
        "root": ""
      }
    },
    "clean": {
      "executor": "@nxrocks/nx-spring-boot:clean",
      "options": {
        "root": ""
      }
    },
    "format": {
      "executor": "@nxrocks/nx-spring-boot:format",
      "options": {
        "root": ""
      }
    },
    "format-check": {
      "executor": "@nxrocks/nx-spring-boot:format-check",
      "options": {
        "root": ""
      }
    }
  },
  "tags": []
}
